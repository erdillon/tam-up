{
    "name": "tam-up",
    "version": "1.0.0",
    "description": "",
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "scripts": {
        "build": "tsc",
        "prisma:generate": "dotenv -- prisma generate",
        "prisma:db-push": "dotenv -- prisma db push",
        "start-local-dependencies": "dotenv -- docker compose -f local-dependencies.docker-compose.yaml up",
        "start-airbyte": "cd airbyte; dotenv -- docker compose -f airbyte.docker-compose.yaml up",
        "generate-urls": "ts-node -r tsconfig-paths/register -r dotenv/config  ./src/bin/generate-urls.ts",
        "start:hosts-resolver": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/bin/start-hosts-resolver.ts",
        "start:worker:local": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/worker.ts",
        "start:worker:nodemon": "nodemon --exec ts-node -r tsconfig-paths/register -r dotenv/config ./src/worker.ts",
        "start:webpages-crawler": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/bin/start-webpages-crawler.ts",
        "run:top-urls": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/bin/domains.ts",
        "run:crawl-one": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/bin/crawl-one.ts",
        "run:load-accounts": "ts-node -r tsconfig-paths/register -r dotenv/config ./src/bin/load-accounts.ts"
    },
    "dependencies": {
        "@cliqz/adblocker-playwright": "^1.26.11",
        "@prisma/client": "^5.5.2",
        "@temporalio/activity": "^1.8.6",
        "@temporalio/client": "^1.8.6",
        "@temporalio/common": "^1.8.6",
        "@temporalio/worker": "^1.8.6",
        "@temporalio/workflow": "^1.8.6",
        "app-root-path": "^3.1.0",
        "axios": "^1.1.2",
        "camelcase-keys": "^9.1.2",
        "cors": "^2.8.5",
        "cross-fetch": "^4.0.0",
        "csv-parse": "^5.5.2",
        "dotenv": "^16.0.3",
        "node-fetch": "^3.2.10",
        "playwright": "^1.39.0",
        "playwright-extra": "^4.3.6",
        "psl": "^1.9.0",
        "puppeteer": "^21.5.0",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "request": "^2.88.2",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "yargs": "^17.6.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/cors": "^2.8.12",
        "@types/node": "^20.8.10",
        "@types/psl": "^1.1.3",
        "@types/yargs": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "6.10.0",
        "dotenv-cli": "^7.0.0",
        "eslint": "^8.0.1",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-standard-with-typescript": "^39.1.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-n": "^16.3.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "nodemon": "^3.0.1",
        "pm2": "^5.2.2",
        "prisma": "^5.5.2",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.2",
        "typescript": "5.2.2"
    }
}